<?xml version="1.0"?>
<component name="fr.toutatice.ecm.platform.webmode.editablewindows.contrib">

	<require>fr.toutatice.ecm.platform.webmode.editablewindows.service</require>

	<extension
		target="fr.toutatice.ecm.platform.webmode.editablewindows.service"
		point="editablewindow">

		<editablewindow code="fgt.html"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.HtmlFragment"
			title="osivia.fragment.html.title" layout="html_fragment_layout" />

		<editablewindow code="fgt.picture"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.PictureFragment"
			title="osivia.fragment.picture.title" layout="picture_fragment_layout" />

		<editablewindow code="fgt.zoom"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.ZoomFragment"
			title="osivia.fragment.zoom.title" layout="zoom_fragment_layout" />

		<editablewindow code="fgt.list"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.ListFragment"
			title="osivia.fragment.list.title" layout="list_fragment_layout" />

		<editablewindow code="fgt.links"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.LinksFragment"
			title="osivia.fragment.links.title" layout="links_fragment_layout" />

		<editablewindow code="fgt.summary"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.SummaryFragment"
			title="osivia.fragment.summary.title" layout="summary_fragment_layout" />

		<editablewindow code="fgt.portlet"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.PortletFragment"
			title="osivia.fragment.portlet.title" layout="portlet_fragment_layout" />

		<editablewindow code="fgt.rss"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.RssFragment"
			title="osivia.fragment.rss.title" layout="rss_fragment_layout" />

		<editablewindow code="fgt.criteria_list"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.CriteriaListFragment"
			title="osivia.fragment.criteria.list.title" layout="criteria_list_fragment_layout" />

		<editablewindow code="fgt.slider_list"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.SliderListFragment"
			title="osivia.fragment.slider.list.title" layout="slider_list_fragment_layout" />

		<editablewindow code="ew.fragment"
			class="fr.toutatice.ecm.platform.service.editablewindows.types.FragmentEditableWindow"
			title="osivia.ew.fragment.title" layout="ew_fragment_layout" />
            
        <editablewindow code="fgt.set"
            class="fr.toutatice.ecm.platform.service.editablewindows.types.SetFragment"
            title="osivia.fragment.set.title" layout="set_fragment_layout" />

	</extension>

</component>
