<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="open-toutatice-addons-web-mode" version="@VERSION@">
	<title>Addon for CMS in osivia portal</title>
	<description>
		<p>Enable the web mode in osivia portal.</p>
	</description>
	<home-page>http://www.toutatice.fr</home-page>
	<vendor>Academie de Rennes - OSIVIA</vendor>
	<installer restart="true" />
	<uninstaller restart="true" />
	<!-- <hotreload-support>true</hotreload-support> -->
	<!-- <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance> -->
	<nuxeo-validation>none</nuxeo-validation>
	<production-state>production_ready</production-state>
	<supported>false</supported>
	<platforms>
		<platform>cap-@CAP_DISTRIB_VERSION@</platform>
		<platform>cap-@CAP_DISTRIB_VERSION@*</platform>
	</platforms>
	<dependencies>
		<package>open-toutatice:@OTTC_VERSION@</package>
	</dependencies>
	<license>LGPL 2.1</license>
	<license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
</package>
